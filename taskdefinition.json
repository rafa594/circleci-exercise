{
  "containerDefinitions": [
    {
      "name": "rafa-service",
      "image": "IMAGENAME",
      "memory": 300,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "rafa-ecs-service-rafa-service",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "rafa-service"
        }
      }
    }
  ],
  "family": "CircleCITaskDefinition"
}